    "url": "https://github.com/JeffersonGarner/React-Boilerplate/issues"
  },
  "homepage": "https://github.com/JeffersonGarner/React-Boilerplate#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "typescript": "^5.0.2",
    "vitest": "^0.29.7"
  },
  "dependencies": {
    "concurrently": "^7.6.0",
    "vite": "^4.2.1"
