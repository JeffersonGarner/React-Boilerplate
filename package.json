{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "vite build",
    "dev": "vite && pnpm watch:all",
    "test": "vitest --ui",
    "test:watch": "vitest --watch",
    "types:watch": "tsc --w --noEmit",
    "test:coverage": "vitest run --coverage",
    "watch:all": "concurrently -n \"TypeScript, Tests\" -c \"cyan.bgBlack, yellow.bgBlack\" \"pnpm types:watch\" \"pnpm run test:watch\" "
  },
